<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Page_Navigation_App.ViewModel"
                    xmlns:view="clr-namespace:ReportServiceProjectWPF.MVVM.Views"
                    xmlns:v="clr-namespace:Page_Navigation_App.View">

    <DataTemplate DataType="{x:Type vm:CategoriesViewModel}">
        <view:CategoriesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DepartmentsViewModel}">
        <view:DepartmentsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EmployeesViewModel}">
        <view:EmployeesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ReportsViewModel}">
        <view:ReportsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StatusViewModel}">
        <view:StatusView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UsersViewModel}">
        <view:UsersView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <v:HomeView/>
    </DataTemplate>

</ResourceDictionary>